{
    "name": "My App",
    "mode": "prod",
    "client": {
        "srcDir": "client/src/",
        "outDir": "public/static/dist/",
        "inputFile": "index.js",
        "port": 5173,
        "startCommandWithInstall": "npm run install-dev",
        "startCommand": "npm run dev",
        "statusCheckEnabled": true,
        "developmentModeEnabled": true
    },
    "router": {
        "baseUrl": "/",
        "errorView": "error"
    },
    "overrides": {
        "namespace": "ExtendRouter\\"
    }
}