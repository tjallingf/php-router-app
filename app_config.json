{
    "name": "My App",
    "mode": "dev",
    "client": {
        "rootDir": "client/",
        "srcDir": "src/",
        "outDir": "../public/static/dist/",
        "inputFile": "index.js",
        "port": 5173,
        "startCommandWithInstall": "npm run install-dev",
        "startCommand": "npm run dev",
        "statusCheckEnabled": true,
        "developmentModeEnabled": false
    },
    "router": {
        "baseUrl": "/",
        "errorView": "error"
    },
    "overrides": {
        "namespace": "ExtendRouter\\"
    }
}